{
  "name": "coding-interview-platform",
  "version": "1.0.0",
  "description": "Online collaborative coding interview platform with real-time code execution",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
